{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "BlogText": {
      "type": "richtext"
    },
    "BlogTitle": {
      "type": "string"
    },
    "BlogCover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "admin_user": {
      "plugin": "admin",
      "model": "user"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "uid": {
      "type": "uid",
      "targetField": "slug"
    }
  }
}
